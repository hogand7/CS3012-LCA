import java.util.ArrayList; 
import java.util.List; 
  
// A Binary Tree node 
class Node { 
    int data; 
    Node left, right; 
  
    Node(int value) { 
        data = value; 
        left = right = null; 
    } 
}

public class LCA {
	
	Node root;
	
	public int findLCA( int node1, int node2){
		if(node1 <= root.data <= node2) return root.data;
		return 0;
	}
}
